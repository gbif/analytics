# kubectl --kubeconfig=/Users/marcoslopezgonzalez/.kube/config cp analytics_regions.tsv gbif-develop/gbif-trino-worker-default-0:/tmp/regions -c trino
# kubectl --kubeconfig=/Users/marcoslopezgonzalez/.kube/config cp analytics_regions.tsv gbif-develop/gbif-trino-coordinator-default-0:/tmp/regions -c trino

DROP TABLE IF EXISTS tmp_regions;

CREATE TABLE tmp_regions (country varchar, gbif_region varchar)
WITH (FORMAT = 'CSV',
    skip_header_line_count = 1,
    csv_separator = U&'\0009',
    EXTERNAL_LOCATION = '/tmp/regions');
